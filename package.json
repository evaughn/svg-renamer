{
  "name": "svg-export-renamer",
  "version": "3.2.3",
  "engines": {
    "sketch": ">=3.0"
  },
  "skpm": {
    "name": "SVG Export Renamer",
    "manifest": "src/manifest.json",
    "main": "SVG Export Renamer.sketchplugin",
    "assets": [
      "assets/**/*"
    ]
  },
  "scripts": {
    "build": "skpm-build",
    "watch": "skpm-build --watch",
    "start": "skpm-build --watch --run",
    "postinstall": "npm run build && skpm-link"
  },
  "devDependencies": {
    "@skpm/builder": "^0.5.11",
    "@skpm/dialog": "^0.2.4"
  },
  "author": "Evan Vaughn <evan@goabstract.com>"
}
